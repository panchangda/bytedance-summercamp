<view class="content"><view class="top-brands"><block tt:for="{{brands}}" tt:for-item="brand" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['onClickBrand',[['o',['id',brand.id],['name',brand.name]]]]]]]}}" bindtap="__e"><view class="top-brand"><image class="icon-brand" src="{{brand.icon}}"></image><view>{{brand.name}}</view></view></view></block></view><text class="title">猜你喜欢</text><view class="box"><view class="guess-cars"><block tt:for="{{guessCars}}" tt:for-item="car" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['onClickCar',[['o',['id',car.id],['name',car.name]]]]]]]}}" bindtap="__e"><view class="guess-car"><image class="image-car" src="{{car.image}}"></image><view>{{car.name}}</view></view></view></block></view></view><u-index-list vue-id="5f8a8c7b-1" scrollTop="{{scrollTop}}" bind:__l="__l" vue-slots="{{['default']}}"><block tt:for="{{indexList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view><u-index-anchor vue-id="{{('5f8a8c7b-2-'+index)+','+('5f8a8c7b-1')}}" index="{{item.letter}}" bind:__l="__l"></u-index-anchor><block tt:if="{{item.data}}"><view><block tt:for="{{item.data}}" tt:for-item="brand" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['onClickBrand',[['o',['id',brand.id],['name',brand.name]]]]]]]}}" bindtap="__e"><view class="indexlist-cell"><image class="icon-brand" src="{{brand.icon}}"></image><view>{{brand.name}}</view></view></view></block></view></block></view></block></u-index-list></view>