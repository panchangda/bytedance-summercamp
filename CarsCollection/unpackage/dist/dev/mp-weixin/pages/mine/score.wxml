<view><view class="tags"><block wx:for="{{tagList}}" wx:for-item="tag" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickTag',[index]]]]]}}" bindtap="__e"><u-tag class="tag" vue-id="{{'60da46d7-1-'+index}}" text="{{tag}}" mode="light" color="#000000" bg-color="{{currentTag==index?'#FFFFFF':'#F7F8FC'}}" border-color="{{currentTag==index?'#606370':'#F7F8FC'}}" shape="square" bind:__l="__l"></u-tag></view></block></view><view class="content"><block wx:for="{{showSeriesList}}" wx:for-item="series" wx:for-index="index"><view><view class="card-series"><image class="card-series-image" src="{{series.image}}"></image><view class="card-series-info"><view class="card-serires-name">{{series.name}}</view><view class="card-serires-stars">{{"综合:"+series.overallStars}}</view><view class="card-serires-stars">{{"我的:"+series.myStars}}</view></view><view data-event-opts="{{[['tap',[['reScore',[index]]]]]}}" class="card-serires-button" bindtap="__e">重新评分</view></view></view></block></view><view><u-popup bind:input="__e" vue-id="60da46d7-2" mode="bottom" height="40%" border-radius="16" value="{{showScore}}" data-event-opts="{{[['^input',[['__set_model',['','showScore','$event',[]]]]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="popup"><image class="popup-close" src="/static/icon_close.png" data-event-opts="{{[['tap',[['closeScorePopup',['$event']]]]]}}" bindtap="__e"></image><view class="popup-title">您觉得本车如何</view><view class="popup-explian">满分5分</view><view data-event-opts="{{[['tap',[['clickScoreSubmit',['$event']]]]]}}" class="popup-submit" bindtap="__e">提交</view></view></u-popup></view></view>